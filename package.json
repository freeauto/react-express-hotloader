{
  "name": "react-express-hotloader",
  "version": "1.0.0",
  "description": "ReactJS, ExpressJS with Hot Loader, Babel",
  "main": "server/index.js",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel server -d dist --presets=es2015,stage-2",
    "serve": "NODE_ENV=production node bin/www.js",
    "start": "nodemon bin/www.js --exec babel-node --presets=es2015,stage-2",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "github.com/freeauto",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2"
  },
  "devDependencies": {}
}
