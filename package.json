{
  "name": "react-express-hotloader",
  "version": "1.0.2",
  "description": "ReactJS, ExpressJS with Hot Loader, Babel",
  "main": "server/index.js",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel server -d dist --presets=es2015,stage-2 && NODE_ENV=production webpack -p",
    "serve": "NODE_ENV=production node bin/www.js",
    "start": "nodemon bin/www.js --exec babel-node --presets=es2015,stage-2",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "github.com/freeauto",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.2.0",
    "webpack-hot-middleware": "^2.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freeauto/react-express-hotloader"
  }
}
